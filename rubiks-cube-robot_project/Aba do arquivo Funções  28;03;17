void topIClockservo(){
  for(char i = 0; i < 30; i++)  servo20graus(); //garra
  for(char i = 0; i < 30; i++) servo90graus();//garra
  delay(100);//
  for(char i = 0; i < 30; i++)  servo20graus();//garra
  for(char i = 0; i < 30; i++) servo90graus();//garra
  delay(100);//garra
  for(char i = 0; i < 30; i++) servo50graus();
  delay(100);//garra
  fim_movimento = 1;//base
  anda_90garra();//base
  delay(100);//base
  
  for(char i = 0; i < 30; i++) servo90graus();
  delay(100);
  for(char i = 0; i < 30; i++)  servo20graus();
  for(char i = 0; i < 30; i++) servo90graus();
  delay(100);
  for(char i = 0; i < 30; i++)  servo20graus();
  for(char i = 0; i < 30; i++) servo90graus();
  delay(100);  
  }
