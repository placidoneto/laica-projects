void leftIClockservo(){//LEFT DONE
  fim_movimento = 1;
  anda_90livre();
  delay(100);
  fim_movimento = 1;
  anda_90livre();
  delay(100);
  fim_movimento = 1;
  anda_90livre();
  delay(100);
  for(char i = 0; i < 30; i++) servo20graus();
  for(char i = 0; i < 30; i++) servo90graus(); 
  delay(100);
  for(char i = 0; i < 30; i++) servo20graus();
  for(char i = 0; i < 30; i++) servo90graus();
  delay(100);
  for(char i = 0; i < 30; i++) servo20graus();
  for(char i = 0; i < 30; i++) servo90graus();
  delay(100);
  for(char i = 0; i < 30; i++) servo50graus();
  delay(100);
  fim_movimento = 1;
  anda_90garra();
  delay(100);
  for(char i = 0; i < 30; i++) servo90graus();
  delay(100);
  for(char i = 0; i < 30; i++) servo20graus();
  for(char i = 0; i < 30; i++) servo90graus();
  fim_movimento = 1;
  anda_90livre();
  delay(100);  
  }
