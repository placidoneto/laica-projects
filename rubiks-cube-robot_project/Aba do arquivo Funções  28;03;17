//METODOS LEFT FORAM TROCADOS POR RIGHT 
//MÃ‰TODOS LEFT E RIGTH TESTADOS E CERTOS :3

void frontIClockservo(){
  for(char i = 0; i < 30; i++)  servo20graus();
  for(char i = 0; i < 30; i++) servo90graus(); 
  delay(100);
  for(char i = 0; i < 30; i++)  servo20graus();
  for(char i = 0; i < 30; i++) servo90graus();
  delay(100);
  for(char i = 0; i < 30; i++)  servo20graus();
  for(char i = 0; i < 30; i++) servo90graus();
  delay(100);
  for(char i = 0; i < 30; i++) servo50graus();
  delay(100);
  fim_movimento = 1;
  anda_90garra();
  delay(100);
  for(char i = 0; i < 30; i++) servo90graus();
  delay(100);
  for(char i = 0; i < 30; i++)  servo20graus();
  for(char i = 0; i < 30; i++) servo90graus();  
  }

void frontClockservo(){
  for(char i = 0; i < 30; i++)  servo20graus();
  for(char i = 0; i < 30; i++) servo90graus(); 
  delay(100);
  for(char i = 0; i < 30; i++)  servo20graus();
  for(char i = 0; i < 30; i++) servo90graus();
  delay(100);
  for(char i = 0; i < 30; i++)  servo20graus();
  for(char i = 0; i < 30; i++) servo90graus();
  delay(100);
  for(char i = 0; i < 30; i++) servo50graus();
  delay(100);
  fim_movimento = 1;
  anda_90garra();
  delay(100);
  fim_movimento = 1;
  anda_90garra();
  delay(100);
  fim_movimento = 1;
  anda_90garra();
  delay(100);
  for(char i = 0; i < 30; i++) servo90graus();
  delay(100);
  for(char i = 0; i < 30; i++)  servo20graus();
  for(char i = 0; i < 30; i++) servo90graus();  
  }

