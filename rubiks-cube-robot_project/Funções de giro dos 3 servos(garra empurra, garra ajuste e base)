void topIClockservo(){//certo7
  for(char i = 0; i < 30; i++)  servo20graus(); //garra
  for(char i = 0; i < 30; i++)  servo90graus();//garra
  delay(200);//
  for(char i = 0; i < 30; i++)  servo20graus();//garra
  for(char i = 0; i < 30; i++)  servo90graus();//garra
  delay(200);//garra
  for(char i = 0; i < 30; i++)  servo50graus();
  delay(200);//garra
  fim_movimento = 1;//base
  anda_90garra();//base
  delay(200);//base
  
  for(char i = 0; i < 30; i++)  servo90graus();
  ajuste_90garra();
  for(char i=0;i<100;i++)servo_ajuste2Fechar();
  for(char i=0;i<100;i++) servo_ajuste2Abrir();
  delay(200);
  for(char i = 0; i < 30; i++)  servo20graus();
  for(char i = 0; i < 30; i++)  servo90graus();
  delay(200);
  for(char i = 0; i < 30; i++)  servo20graus();
  for(char i = 0; i < 30; i++)  servo90graus();
  for(char i=0;i<100;i++)servo_ajuste2Fechar();
